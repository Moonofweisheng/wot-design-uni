  "browserslist": [
    "Android >= 4.4",
    "ios >= 9"
  ],
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix --ext .js,.vue,.ts src"
  },
  "uni-app": {
    "scripts": {
      "mp-dingtalk": {
        "title": "钉钉小程序",
        "env": {
          "UNI_PLATFORM": "mp-alipay"
        },
        "define": {
          "MP-DINGTALK": true
        }
