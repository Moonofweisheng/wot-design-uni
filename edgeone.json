{
  "redirects": [
    {
      "source": "$wwwhost",
      "destination": "$host",
      "statusCode": 301
    }
  ],
  "headers": [
    {
      "source": "/*.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain; charset=utf-8"
        }
      ]
    }
  ]
}
